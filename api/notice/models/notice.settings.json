{
  "connection": "default",
  "collectionName": "notices",
  "info": {
    "name": "notice"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "datetime": {
      "type": "datetime"
    },
    "location": {
      "type": "string"
    }
  }
}
